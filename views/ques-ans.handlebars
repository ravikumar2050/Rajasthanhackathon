 <script>
var counter=1;
$('#next').on('click',function(event)
{console.log(counter)
	event.preventDefault();

event.stopPropagation();
var data={};
data.quiztype=$("#quiztype").html();
console.log(data)
$.ajax({
  type: 'post',
  url: "/users/next",
  data:data,
  dataType:'json'
}) // At this point the request is sent to the server
// and the client uses the response it gets down here
  .done(function(response) {
	  console.log(response);
    populate(response);
  })
.fail(function(error)
{
	console.log('no more')

}
)
})
 function populate(data)
{
	if(data.length<=counter)
	{
$('#test').html("no more Question")

$('#alert').hide()
	}
	else{
    $('#sub').show()
    $('#alert').hide()
console.log("data found")
$('#question').html(data[counter].question)
$('#op1').html(data[counter].op1)
$('#op2').html(data[counter].op2)
$('#op3').html(data[counter].op3)
$('#op4').html(data[counter].op4)
$('#a1').attr('value',(data[counter].op1))
$('#a2').attr('value',(data[counter].op2))
$('#a3').attr('value',(data[counter].op3))
$('#a4').attr('value',(data[counter].op4))
counter++;}
}

	 </script>


   <script>
        $("#sub").on('click',function(event) {
                event.preventDefault();
                var data = {};
                data.quiztype=$("#quiztype").html();
               data.question=$('#question').html();
                data.ans = $('input[name=q1]:checked').val();
                console.log(data)
               $('#sub').hide()
                $.ajax({
                    type: 'post',
                    url: '/users/sub',
                    data: data,
                    dataType: 'json',
                   
                    }).done(function(response) {
	  fill(response)
    
  })
.fail(function(error)
{
	console.log('no more')

}
)
function fill(response)
{ 
    $('#alert').show()


  if(response==true)
  { $('#alert').attr('class','alert alert-success')
$('#alert').html("Right Answer")
  }
  else{
  $('#alert').attr('class','alert alert-danger')
  
    $('#alert').html("Wrong Answer")
  }
}
                });
            
   </script>


